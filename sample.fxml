<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Pane prefHeight="300.0" prefWidth="300.0">
            <children>
                <Button fx:id="buttonEnter" layoutX="115.0" layoutY="210.0" mnemonicParsing="false" onAction="#Enter" prefHeight="25.0" prefWidth="80.0" text="Enter" />
                <TextField fx:id="txtLogin" layoutX="105.0" layoutY="70.0" />
                <TextField fx:id="txtPass" layoutX="105.0" layoutY="120.0" />
                <Label fx:id="labelText" layoutX="110.0" layoutY="180.0" text="Type the login and password" />
                <Label fx:id="labelPass" layoutX="33.0" layoutY="125.0" text="password" />
                <Label fx:id="labelLogin" layoutX="45.0" layoutY="70.0" text="login" />
            </children></Pane>
    </children>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
</GridPane>